{
  "manifest_version": 3,
  "name": "Netflix Ratings",
  "description": "Provides Ratings to Netflix Shows",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["https://*.netflix.com/*"],
      "css": ["main.css"],
      "js": ["config.js", "contentScript.js"]
    }
  ],
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Netflix Ratings!"
  }
}
